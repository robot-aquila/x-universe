<line linenr="004" interruptable="@" indent=""><var>$vnStations&#160;=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;call&#160;script&#160;</text><call>wwsl.T2.getVarName</call><text>&#160;:&#160;</text><text>&#160;Variable&#160;suffix=</text><var>'LS.hash'</var></line>
<line linenr="005" indent=""><var>$StationsHash&#160;=&#160;</var><text>get&#160;global&#160;variable:&#160;name=</text><var>$vnStations</var></line>
<line linenr="006" indent=""><var>skip&#160;if&#160;</var><var>$StationsHash</var></line>
<line linenr="007" indent="&#160;"><text>return&#160;</text><var>[FALSE]</var></line>
