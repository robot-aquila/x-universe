<line linenr="016" indent="&#160;&#160;"><text>dec&#160;</text><var>$maxX&#160;=&#160;</var></line>
<line linenr="017" indent="&#160;&#160;"></line>
<line linenr="018" indent="&#160;&#160;"><text>inc&#160;</text><var>$counter&#160;=&#160;</var></line>
<line linenr="019" indent="&#160;&#160;"><var>if&#160;</var><var>$counter</var><text>&#160;</text><var>&gt;=</var><text>&#160;</text><var>20</var></line>
<line linenr="020" indent="&#160;&#160;&#160;"><var>$counter&#160;=&#160;</var><var>0</var></line>
